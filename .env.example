# MCP Orchestration System Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
POSTGRES_DB=mcp_unified
POSTGRES_USER=mcp_admin
POSTGRES_PASSWORD=change_me_in_production
MCP_DATABASE_URL=postgresql://mcp_admin:change_me_in_production@postgresql:5432/mcp_unified
DEFAULT_POSTGRES_URL=postgresql://mcp_admin:change_me_in_production@postgresql:5432/mcp_unified

# Redis Configuration
REDIS_URL=redis://redis:6379

# API Keys (get your keys from respective providers)
GEMINI_API_KEY=your_gemini_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# MCP Server Configuration
MCP_SERVER_PORT=8000
QDRANT_URL=http://qdrant-vector:6333